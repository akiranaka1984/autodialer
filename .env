# .env
# モックモード設定 (true=Asteriskサーバーなしでテスト, false=実際のAsteriskサーバーに接続)
MOCK_ASTERISK=true

# 実際のAsterisk接続情報 (MOCK_ASTERISK=falseの場合に使用)
ASTERISK_HOST=localhost
ASTERISK_PORT=5038
ASTERISK_USERNAME=admin
ASTERISK_PASSWORD=password

# データベース設定 (必要に応じて)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=autodialer

# ログレベル設定
LOG_LEVEL=info